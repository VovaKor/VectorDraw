<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<TitledPane xmlns:fx="http://javafx.com/fxml/1"
            layoutX="10.0" xmlns="http://javafx.com/javafx/8"
            text="Position Z"
            fx:controller="vectordraw.controllers.ShapePositionZController" fx:id="mainPane"
            style="-fx-border-color:lightgray; -fx-border-width: 1; -fx-border-style: solid; -fx-border-radius: 5; -fx-border-insets: -3;">
<AnchorPane>
    <Button fx:id="foregroundB" layoutX="0.0" layoutY="0.0">
        <graphic>
            <ImageView>
                <Image url="@/res/foreground.png"/>
            </ImageView>
        </graphic>
        <tooltip>
            <Tooltip text="%PositionZPaneTooltip.1"/>
        </tooltip>
    </Button>
    <Button fx:id="backgroundB" layoutX="40.0" layoutY="0.0">
        <graphic>
            <ImageView>
                <Image url="@/res/background.png"/>
            </ImageView>
        </graphic>
        <tooltip>
            <Tooltip text="%PositionZPaneTooltip.2"/>
        </tooltip>
    </Button>
</AnchorPane>
</TitledPane>